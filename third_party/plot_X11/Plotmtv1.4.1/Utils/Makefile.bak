#
# Conversion utilities
#
CONV1_SOURCES = ctr2mtv.c
CONV1_OBJECTS = ctr2mtv.o
CONV1_PROGRAM = ctr2mtv

CONV2_SOURCES = drawplot2mtv.c
CONV2_OBJECTS = drawplot2mtv.o
