	$(CC) -o $@ $(CONV2_OBJECTS) $(LDOPTIONS)  $(PLOTLIB) $(LDLIBS)   $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) $(CONV2_PROGRAM)

