#
# Test various marker-types  
#
$ DATA=CURVE2D

# Set Global properties for the curves in the dataset
% toplabel   = "Marker Types"                          # Top label for plot
% subtitle   = "Linetype=0"                            # Subtitle
% comment    = "MTV provides 13 different marker-types"# Comment
% dlinetype  = 0                                       # Markertype
% yflip=true                                           # y-axis is flipped
% xmin=0.0 xmax=1.0 ymin=-0.25 ymax=4.25               # Plot boundaries

# Curve 0
% markertype = 0
% linelabel  = "Marker 0"
@ text x1=0.07 y1=0.00 linelabel="0"
  0.10   0.00
  0.90   0.00

